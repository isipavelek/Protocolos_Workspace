#ifndef API_INC_API_RELOJ_H_
#define API_INC_API_RELOJ_H_

typedef struct reloj{

	uint8_t seg;
	uint8_t min;
	uint8_t hora;
	uint8_t diasem;
	uint8_t dia;
	uint8_t mes;
	uint8_t anio;

};

void RelojInit(void);
void PresentaHora(void);


#endif /* API_INC_API_RELOJ_H_ */
